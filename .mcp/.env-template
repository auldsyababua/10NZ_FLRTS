# Project-specific MCP API keys
# Copy this file to .env and fill in your values

# Todoist
TODOIST_API_TOKEN=your_todoist_api_token_here

# Google Drive
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Noloco
NOLOCO_API_KEY=your_noloco_api_key_here
NOLOCO_API_URL=your_noloco_api_url_here
# Add any additional Noloco parameters needed for your specific setup
# NOLOCO_PROJECT_ID=your_noloco_project_id_here
# NOLOCO_USER_ID=your_noloco_user_id_here

# Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_BOT_NAME=your_telegram_bot_name_here
TELEGRAM_BOT_USERNAME=your_telegram_bot_username_here

# Supabase
SUPABASE_ACCESS_TOKEN=your_supabase_access_token_here
SUPABASE_READ_ONLY=true  # Set to "false" to allow write operations